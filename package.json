{
  "name": "rearm",
  "version": "0.13.1",
  "description": "powerful react.js abstractions for non-trivial apps",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "docs-dev": "webpack-dev-server --mode=development --port 9398 --config ./src/docs/webpack.config.js",
    "build-docs": "webpack --mode=production --config ./src/docs/webpack.config.js && node ./scripts/buildDocs.js",
    "build": "env NODE_ENV=production babel src --out-dir lib --ignore src/docs,src/__tests__",
    "prepare": "npm run build",
    "gh-pages": "npm run build-docs && gh-pages -d docs-pub",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brigand/rearm.git"
  },
  "keywords": [
    "react",
    "abstraction",
    "hoc",
    "highordercomponent",
    "rendercallback"
  ],
  "author": "Frankie Bagnardi <f.bagnardi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brigand/rearm/issues"
  },
  "homepage": "https://github.com/brigand/rearm#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^0.28.11",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.14.3",
    "flow-bin": "^0.74.0",
    "gh-pages": "^1.2.0",
    "highlight.js": "^9.12.0",
    "jest": "^24.9.0",
    "marked": "^0.7.0",
    "mkdirp": "^0.5.1",
    "outdent": "^0.5.0",
    "peacock": "^0.3.1",
    "prettier": "1.13.5",
    "raw-loader": "^0.5.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.9.0",
    "showdown": "^1.8.6",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.7",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "add-resize-listener": "^2.0.1",
    "prop-types": "^15.6.1"
  }
}
