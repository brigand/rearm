{
  "name": "rearm",
  "version": "0.10.0",
  "description": "powerful react.js abstractions for non-trivial apps",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "docs-dev": "webpack-dev-server --port 9398 --config ./src/docs/webpack.config.js",
    "build-docs": "webpack --config ./src/docs/webpack.config.js && node ./scripts/buildDocs.js",
    "build": "env NODE_ENV=production babel src --out-dir lib --ignore src/docs,src/__tests__",
    "prepare": "npm run build",
    "gh-pages": "npm run build-docs && gh-pages -d docs-pub",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brigand/rearm.git"
  },
  "keywords": [
    "react",
    "abstraction",
    "hoc",
    "highordercomponent",
    "rendercallback"
  ],
  "author": "Frankie Bagnardi <f.bagnardi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brigand/rearm/issues"
  },
  "homepage": "https://github.com/brigand/rearm#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.57.3",
    "gh-pages": "^1.2.0",
    "highlight.js": "^9.12.0",
    "jest": "^23.1.0",
    "marked": "^0.3.6",
    "mkdirp": "^0.5.1",
    "outdent": "^0.3.0",
    "peacock": "^0.3.1",
    "prettier": "1.13.5",
    "raw-loader": "^0.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "16.4.0",
    "showdown": "^1.8.0",
    "style-loader": "^0.21.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "dependencies": {
    "add-resize-listener": "^2.0.1",
    "prop-types": "^15.6.0"
  }
}
